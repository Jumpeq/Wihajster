@model Wihajster.Models.Picture

@{
    ViewBag.Title = "Szczegóły";
}

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            Nazwa
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edytuj", "Edit", new { id = Model.PictureId }, new { @class = "btn btn-warning" })
    @Html.ActionLink("Usuń", "Delete", new { id = Model.PictureId}, new { @class = "btn btn-danger" })
    @Html.ActionLink("Powrót do listy", "Index", null, new { @class = "btn btn-primary" })
</p>
